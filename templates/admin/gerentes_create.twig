{% extends 'dashbase.twig' %}

{% block title %}Painel do admin{% endblock %}
{% block body %}
    <div class="container form-gerente mt-5">
    <form action="" method="post">
            <h3 class="text-light">Registrar Gerente</h3>

            <div class="row">
            <div class="col-md-6">
            <div class="form-group">
                <label for="cpf">CPF</label>
                <input type="text" class="form-control" id="cpf" name="cpf" placeholder="CPF">
            </div>
        </div>
        <div class="col-md-6">
            <div class="form-group">
                <label for="nome">Nome do Gerente</label>
                <input type="text" class="form-control" id="nome" name="nome" placeholder="Nome">
            </div>
        </div>

        </div>

        <div class="row">
            <div class="col-md-6">
            <div class="form-group">
                <label for="telefone">Telefone</label>
                <input type="text" class="form-control" id="telefone" name="celular" placeholder="Telefone">
            </div>
        </div>
        <div class="col-md-6">
            <div class="form-group">
                <label for="email">Email</label>
                <input type="email" class="form-control" id="email" name="email" placeholder="Email">
            </div>
        </div>

        </div>

            <div class="form-group">
                <label for="senha">Senha</label>
                <input type="password" class="form-control" id="senha" name="password" placeholder="Senha">
            </div>
            

            <button type="submit" class="btn btn-primary mt-3">Cadastrar</button>

    </form>
       
    </div>

    

{% endblock %}
